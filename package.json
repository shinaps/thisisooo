{
	"name": "thisisooo",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --env development",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
		"better-auth:generate": "npx @better-auth/cli@latest generate --output ./src/drizzle/schemas.ts --yes",
		"drizzle:generate": "npx drizzle-kit generate",
		"drizzle:migrate": "npx drizzle-kit migrate"
	},
	"dependencies": {
		"@libsql/client": "^0.15.10",
		"@opennextjs/cloudflare": "^1.3.0",
		"@t3-oss/env-nextjs": "^0.13.8",
		"better-auth": "^1.3.4",
		"dotenv": "^17.2.1",
		"drizzle-orm": "^0.44.4",
		"next": "15.3.5",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"zod": "^4.0.14"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "^4",
		"@types/node": "^20.19.9",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"drizzle-kit": "^0.31.4",
		"tailwindcss": "^4",
		"typescript": "^5",
		"wrangler": "^4.26.0"
	}
}
